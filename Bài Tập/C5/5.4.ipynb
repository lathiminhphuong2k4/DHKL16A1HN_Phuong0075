    "import sqlite3\n",
    "conn=sqlite3.connect(\"mydatabase.db\")\n",
    "\n",
    "cursor = conn.cursor()\n",
    "\n",
    "cursor.execute(\"SELECT name FROM sqlite_master WHERE type='table';\")\n",
    "tables = cursor.fetchall()[0]\n",
    "\n",
    "#in\n",
    "for table in tables:\n",
    "    print(table)\n",
    "\n",
    "# Đóng kết nối và cursor\n",
    "cursor.close()\n",
    "conn.close()"
   ]
  }
